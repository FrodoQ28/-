using System;

namespace ДЗ_Последовательность
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int startNumber = 5;
            int endNumber = 110;
            int plusNumber = 7;

            for (int i = startNumber; i != endNumber; i += plusNumber)
            {
                Console.WriteLine(i);
            }
        }
    }
}
